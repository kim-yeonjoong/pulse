{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "baseUrl": "./",
    "rootDir": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true
  }
}

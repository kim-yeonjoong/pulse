{
  "name": "@yeonjoong/pulse",
  "version": "1.1.1",
  "description": "Keeping the pulse of your APIs under watch.",
  "author": "Kim YeonJoong <duswnd25@gmail.com> (https://yeonjoong.dev)",
  "type": "module",
  "homepage": "https://github.com/kim-yeonjoong/pulse",
  "repository": {
    "url": "git+https://github.com/kim-yeonjoong/pulse.git"
  },
  "scripts": {
    "dev": "jiti lib/index.ts",
    "build": "tsup",
    "lint": "eslint lib --config ./eslint.config.mjs --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "sample": "jiti lib/index.ts sample.json"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pulse": "dist/index.js"
  },
  "files": [
    "!lib",
    "dist"
  ],
  "keywords": [
    "status",
    "check",
    "pulse",
    "healthcheck"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "license": "MIT",
  "dependencies": {
    "@libsql/client": "^0.15.4",
    "commander": "^13.1.0",
    "confbox": "^0.1.8",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "drizzle-orm": "^0.41.0",
    "es-toolkit": "^1.34.1",
    "ky": "^1.8.0",
    "ora": "^8.2.0",
    "ufo": "^1.5.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/eslint-plugin": "^1.1.39",
    "eslint": "9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vitest": "^3.1.1"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
